<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <FrameLayout
        android:id="@+id/tmappinContainer"
        android:layout_width="match_parent"
        android:layout_height="match_parent" />

        
    <androidx.appcompat.widget.Toolbar
        android:id="@+id/toolbar"
        android:layout_width="match_parent"
        android:layout_height="60dp"
        android:background="@android:color/white">
        <TextView
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:text="주변 전기차충전소"
            android:textColor="@android:color/black"
            android:gravity="center"
            android:textSize="24sp"
            android:textStyle="bold"
            android:fontFamily="inter"
        /> 

    </androidx.appcompat.widget.Toolbar>


    <ImageButton
            android:id="@+id/findbutton"
            android:layout_width="201dp"
            android:layout_height="66dp"
            android:background="@drawable/findbutton"
            app:layout_constraintTop_toTopOf="@id/tmappinContainer"
            app:layout_constraintStart_toStartOf="@id/tmappinContainer"
            app:layout_constraintEnd_toEndOf="@id/tmappinContainer"
            app:layout_constraintBottom_toBottomOf="@id/tmappinContainer"
            android:onClick="onClickFind"
            app:layout_constraintVertical_bias="0.9"
            app:layout_constraintHorizontal_bias="0.5" />

</androidx.constraintlayout.widget.ConstraintLayout>